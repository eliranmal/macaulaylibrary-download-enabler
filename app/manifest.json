{
    "name": "sample downloader",
    "description": "easy downloader for audio samples",
    "version": "0.1.0",
    "permissions": [
        "tabs",
        "downloads",
        "contextMenus",
        "http://macaulaylibrary.org/"
    ],
    "background": {
        "scripts": [
            "app.js",
            "background.js"
        ]
    },
    "content_scripts": [{
        "matches": [
            "http://macaulaylibrary.org/*"
        ],
        "js": [
            "lib/jquery/jquery-2.2.0.min.js",
            "app.js",
            "macaulay-library.js"
        ],
        "run_at": "document_end"
    }],
    "manifest_version": 2
}
